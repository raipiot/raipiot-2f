    "type": "git",
    "url": "https://github.com/raipiot/raipiot-2f.git",
    "directory": "packages/hooks"
  },
  "keywords": [
    "raipiot",
    "hooks"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./meta": "./dist/meta.mjs"
  },
  "types": "./src/index.ts",
  "scripts": {
    "build:hooks:meta": "tsup",
