{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true,
      "dotEnv": [".env.development.local", ".env.development", ".env.local", ".env"]
    },
    "@raipiot-2f/admin#dev": {
      "dependsOn": ["@raipiot-2f/hooks#build:hooks:meta"],
      "cache": false,
      "persistent": true,
      "dotEnv": [".env.development.local", ".env.development", ".env.local", ".env"]
    },
    "build:hooks:meta": {
      "dependsOn": ["^type:check", "^build"],
      "outputs": ["dist/**"]
    },
    "build:staging": {
      "dependsOn": ["^type:check", "^build", "@raipiot-2f/hooks#build:hooks:meta"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "dotEnv": [".env.staging.local", ".env.staging", ".env.local", ".env"]
    },
    "build:prod": {
      "dependsOn": ["^type:check", "^build", "@raipiot-2f/hooks#build:hooks:meta"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "dotEnv": [".env.production.local", ".env.production", ".env.local", ".env"]
    },
    "preview": {
      "cache": false,
      "persistent": true,
      "dotEnv": [".env.development.local", ".env.development", ".env.local", ".env"]
    },
    "clean": {},
    "desktop:dev": {
      "cache": false,
      "persistent": true,
      "dotEnv": [".env.development.local", ".env.development", ".env.local", ".env"]
    },
    "desktop:build:staging": {
      "dependsOn": ["^type:check", "^desktop:dev"],
      "outputs": ["dist/**", "target/**"],
      "dotEnv": [".env.staging.local", ".env.staging", ".env.local", ".env"]
    },
    "desktop:build:prod": {
      "dependsOn": ["^type:check", "^desktop:dev"],
      "outputs": ["dist/**", "target/**"],
      "dotEnv": [".env.production.local", ".env.production", ".env.local", ".env"]
    },
    "test": {
      "cache": false,
      "persistent": true
    },
    "test:unit": {
      "cache": false,
      "persistent": true
    },
    "test:e2e": {
      "cache": false,
      "persistent": true
    },
    "test:coverage": {
      "cache": false,
      "persistent": true,
      "outputs": ["coverage/**"]
    },
    "cspell:sort:dict": {},
    "cspell:check": {},
    "type:check": {},
    "prettier:check": {},
    "prettier:fix": {},
    "eslint:check": {},
    "eslint:fix": {}
  }
}
