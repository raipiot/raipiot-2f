{
  "name": "raipiot-2f",
  "description": "raipiot 2F Saas 平台",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "scripts": {
    "dev:admin": "turbo dev --filter=@raipiot-2f/admin",
    "dev:shopping": "turbo dev --filter=@raipiot-2f/shopping",
    "dev:mock": "turbo dev --filter=@raipiot-2f/mock",
    "build:admin:staging": "turbo build:staging --filter=@raipiot-2f/admin",
    "build:shopping:staging": "turbo build:staging --filter=@raipiot-2f/shopping",
    "build:admin:prod": "turbo build:prod --filter=@raipiot-2f/admin",
    "build:shopping:prod": "turbo build:prod --filter=@raipiot-2f/shopping",
    "preview:admin": "turbo preview --filter=@raipiot-2f/admin",
    "preview:shopping": "turbo preview --filter=@raipiot-2f/shopping",
    "desktop:dev:admin": "turbo desktop:dev --filter=@raipiot-2f/admin",
    "desktop:dev:shopping": "turbo desktop:dev --filter=@raipiot-2f/shopping",
    "desktop:build:staging:admin": "turbo desktop:build:staging --filter=@raipiot-2f/admin",
    "desktop:build:staging:shopping": "turbo desktop:build:staging --filter=@raipiot-2f/shopping",
    "desktop:build:prod:admin": "turbo desktop:build:prod --filter=@raipiot-2f/admin",
    "desktop:build:prod:shopping": "turbo desktop:build:prod --filter=@raipiot-2f/shopping",
    "start:mock": "pnpm --filter=@raipiot-2f/mock build &&pnpm --filter=@raipiot-2f/mock pm2:start",
    "clean": "turbo clean",
    "test:admin": "turbo test --filter=@raipiot-2f/admin",
    "test:unit:admin": "turbo test:unit --filter=@raipiot-2f/admin",
    "test:coverage:admin": "turbo test:coverage --filter=@raipiot-2f/admin",
    "test:e2e:admin": "turbo test:e2e --filter=@raipiot-2f/admin",
    "gen:package": "turbo gen workspace --copy @raipiot-2f/template --type package",
    "cspell:sort:dict": "turbo cspell:sort:dict --filter=@raipiot-2f/cspell",
    "cspell:check": "turbo cspell:check",
    "type:check": "turbo type:check",
    "prettier:check": "turbo prettier:check",
    "prettier:fix": "turbo prettier:fix",
    "eslint:check": "turbo eslint:check",
    "eslint:fix": "turbo eslint:fix",
    "cz": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.1.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@raipiot-2f/cspell": "workspace:^",
    "@raipiot-infra/commitlint-config": "~0.1.13",
    "@raipiot-infra/cspell": "~0.1.13",
    "@raipiot-infra/eslint-config": "~0.1.13",
    "@raipiot-infra/eslint-config-ts": "~0.1.13",
    "@raipiot-infra/prettier": "~0.1.13",
    "@raipiot-infra/rpx": "~0.1.13",
    "@raipiot-infra/tsconfig": "~0.1.13",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "cspell": "^8.6.0",
    "cz-git": "^1.9.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "pm2": "^5.3.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "turbo": "^1.12.5",
    "typescript": "~5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "packageManager": "pnpm@8.0.0",
  "engines": {
    "node": ">=20"
  },
  "private": true
}
